{
    "name": "@wingtechbot-mk3/types",
    "version": "1.0.0",
    "description": "Shared TypeScript types for WingTechBot MK3 API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsc && node scripts/generate-exports.js",
        "dev": "tsc --watch",
        "clean": "rimraf dist"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "eslint": "^9.30.1",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "zod": "^4.0.2"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./api/v1/users": {
            "types": "./dist/api/v1/users.d.ts",
            "import": "./dist/api/v1/users.js"
        },
        "./common/api": {
            "types": "./dist/common/api.d.ts",
            "import": "./dist/common/api.js"
        },
        "./common/error": {
            "types": "./dist/common/error.d.ts",
            "import": "./dist/common/error.js"
        }
    }
}
